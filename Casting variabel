class Main {
    public static void main(String[] args) {
        int x=1;
        double y=x;
        System.out.println("Nilai x : "+x);
        System.out.println("Nilai y : "+y);
        
        double z=20.5;
        int w=(int)z;
        System.out.println("Nilai z : "+z);
        System.out.println("Nilai w : "+w);
    }
}
