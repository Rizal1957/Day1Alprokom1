import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner s=new Scanner(System.in);
	    System.out.print("Jumlah peserta yang akan diambil dari region 1 adalah : ");
	    int k=s.nextInt();
	    System.out.print("Total peserta yang diambil adalah : ");
	    int n=s.nextint();
	    int N=2290;
	    int M=878;
	    int NM=N-M;
	    int nk=n-k;
	    
	    long x=kombinasi 878 dan k;
	    long y=kombinasi N-M dan n-k;
	    long z= kombinasi N dan n;
        double p=x*y/z;
        
        System.out.println("Total Semifinalis : ");
        System.out.println("Total Semifinalis dari region 1 : ");
        System.out.println("Total diambilnya "+k+"peserta dari region 1 adalah : "+p);
	}
}
