class Main {
    
    public static void main(String[] args) {
        
        int C=29;
        int K=C+273;
        double R=4.0*C/5;
        double F=(9.0*C/5)+32;
        
        System.out.println("Suhu dalam celsius : " +C);
        System.out.println("Suhu dalam Kelvin : " +K);
        System.out.println("Suhu dalam Reamur : " +R);
        System.out.println("Suhu dalam Fahrenheit : " +F);
    }
    
}
