import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);

        System.out.print("Masukkan sebuah nilai : ");
        int n = s.nextInt();

        System.out.println("Maka akar kuadratnya : " + kuadrat(n));
        System.out.println("Maka akar kubiknya   : " + kubik(n));
    }

    static String kuadrat(int x) {
        for (int i = 0; i*i <= x; i++) {
            if (i * i == x) {
                return Integer.toString(i);
            }
        }
        return "Irrasional";
    }

    static String kubik(int y) {
        for (int i = 0; i*i*i <= y; i++) {
            if (i * i * i == y) {
                return Integer.toString(i);
            }
        }
        return "Irrasional";
    }
}
