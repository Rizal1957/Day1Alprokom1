public class Main
{
	public static void main(String[] args) {
	    int x=7;
		System.out.println("Dalam method main x : "+x);
		test1();
		System.out.println("Dalam method main : nilai baru x : "+x);
	}
	
	public static void test1(){
	    int x=2;
	    test2(x);
	    System.out.println("Dalam method test1 : nilai x : "+x);
	}
	
	public static void test2(int y){
	    int x=8/y;
	    System.out.println("Dalam method test2 : nilai x : "+x);
	}
}
