import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		System.out.print("Anda akan memasukkan berapa nilai? ");
		int n=s.nextInt();
		int[]angka=new int[n];
		System.out.print("Masukkan angka sebanyak "+n+" : ");
		for(int i=0;i<n;i++){
		    angka[i]=s.nextInt();
		}
		
		int indeks=CariIndeksMin(angka);
		System.out.println("Maka angka terendah dari beberapa angka yang kamu masukkan berada pada urutan : "+indeks);
	}
	
	public static int CariIndeksMin(int[]x){
	    int hasil=x[0];
	    int index=0;
	    for(int i=1;i<x.length;i++){
	        if(x[i]<hasil){
	            hasil=x[i];
	            index=++i;
	        }
	    }
	    return index;
	}
}
