import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		System.out.print("Anda akan memasukkan berapa nilai? ");
		int n=s.nextInt();
		int[]angka=new int[n];
		System.out.print("Masukkan angka sebanyak "+n+" : ");
		for(int i=0;i<n;i++){
		    angka[i]=s.nextInt();
		}
		int indeks=0;
	    int hasil=angka[0];
		for(int i=1;i<n;i++){
		    if(angka[i]<hasil){
		        hasil=angka[i];
		        indeks=++i;
		    }
		}
		
		System.out.println("Maka angka terendah dari beberapa angka yang kamu masukkan berada pada urutan : "+indeks);
	}
}
